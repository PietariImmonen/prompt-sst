{
  "name": "@sst-replicache-template/desktop",
  "version": "0.0.0",
  "description": "Electron desktop shell for Prompt SST",
  "main": "./out/main/index.js",
  "type": "module",
  "scripts": {
    "format": "bunx prettier --write .",
    "lint": "bunx eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx",
    "typecheck:node": "bunx tsc --noEmit -p tsconfig.node.json",
    "typecheck:web": "bunx tsc --noEmit -p tsconfig.web.json",
    "typecheck": "bun run typecheck:node && bun run typecheck:web",
    "typecheck:sst": "cd ../.. && sst bind -- bun run --filter @sst-replicache-template/desktop typecheck",
    "env:generate": "node ../../scripts/generate-desktop-env.mjs",
    "start": "bunx electron-vite preview",
    "dev": "bunx electron-vite dev",
    "dev:with-env": "bun run env:generate && bun run dev",
    "build": "bun run env:generate -- --stage production --mode production && bun run typecheck && bunx electron-vite build",
    "postinstall": "bunx electron-builder install-app-deps",
    "build:unpack": "bun run build && bunx electron-builder --dir",
    "build:win": "bun run build && bunx electron-builder --win",
    "build:mac": "bun run build && bunx electron-builder --mac",
    "build:linux": "bun run build && bunx electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@openauthjs/openauth": "^0.4.3",
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sst-replicache-template/core": "workspace:*",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "electron-updater": "^6.1.7",
    "embla-carousel-react": "^8.6.0",
    "i18next": "^23.0.0",
    "i18next-browser-languagedetector": "^8.0.0",
    "lucide-react": "^0.456.0",
    "mqtt": "^5.10.4",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^15.0.0",
    "react-router-dom": "^6.28.0",
    "replicache": "^15.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@tailwindcss/postcss": "^4.0.15",
    "@tailwindcss/vite": "^4.0.15",
    "@types/node": "^20.17.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "autoprefixer": "^10.4.20",
    "electron": "31.0.2",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "postcss": "^8.4.49",
    "prettier": "^3.3.2",
    "tailwindcss": "^4.0.15",
    "typescript": "^5.5.2",
    "vite": "^5.3.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild"
    ]
  }
}
