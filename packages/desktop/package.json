{
  "name": "@prompt-saver/desktop",
  "version": "0.1.58",
  "description": "Electron desktop shell for Clyo",
  "main": "./out/main/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/PietariImmonen/prompt-sst.git",
    "directory": "packages/desktop"
  },
  "author": {
    "name": "Pietari Immonen",
    "email": "pietari@example.com"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
    "typecheck:sst": "cd ../.. && sst bind -- pnpm --filter @prompt-saver/desktop typecheck",
    "env:generate": "node ../../scripts/generate-desktop-env.mjs",
    "setup:dev-permissions": "bash ./scripts/setup-dev-permissions.sh",
    "start": "pnpm dlx electron-vite preview",
    "dev": "electron-vite dev",
    "build": "pnpm run typecheck && electron-vite build",
    "rebuild": "pnpm dlx electron-rebuild",
    "build:unpack": "pnpm run build && electron-builder --dir",
    "build:win": "pnpm run build && electron-builder --win",
    "build:mac": "rm -rf dist && pnpm run build && electron-builder --mac",
    "build:mac:notarized": "./scripts/build-mac-notarized.sh",
    "build:linux": "pnpm run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron/notarize": "^3.1.0",
    "@noble/hashes": "^1.3.3",
    "@openauthjs/openauth": "^0.4.3",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prompt-saver/core": "workspace:*",
    "@radix-ui/react-accordion": "^1.2.12",
